{% extends "base.twig" %}

{% block title %}Page Not Found{% endblock %}

{% block styles %}
    {{ parent() }}
    <link rel="stylesheet" href="404.css">
{% endblock %}

{% block content %}
    <h1>404 - Page Not Found</h1>
    <p>Sorry, the page you are looking for does not exist.</p>
    <a href="/">Return to Home</a>
{% endblock %}

{% block scripts %}
    {{ parent() }}
    <script src="404.js"></script>
{% endblock %}